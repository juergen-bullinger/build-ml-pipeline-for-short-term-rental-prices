name: test_regression_model
conda_env: conda.yml

entry_points:
  main:
    parameters:

      mlflow_model:
        description: Trained random forest model marked for production
        type: string

      test_artifact:
        description: Test data to perform a test on the production model
        type: string


    command: >-
        python run.py \
            --mlflow_model {mlflow_model} \
            --test_artifact {test_artifact} 
